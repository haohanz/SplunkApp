  <row>
    <panel>
      <title>Most Recent 10 Events for Model</title>
      <html>
        <head>
          <style type="text/css">
            #modelSelector {
              width: 20%;
              min_width: 100px;
              height: 38px;
              vertical-align: top;
            }
            
            #applyModelButton {
              width: 60px;
              height: 38px;
              background-color: LightGray;
            }
            
            #note {
              padding-left: 80px;
            }
          </style>
        </head>
        <body>
          <div>
            <select id="modelSelector">
              <option>Select Model</option>
            </select>
            <button id="applyModelButton">Apply</button>
            <h id="note">Note: "most recent events" means the latest events for a histoical search, or the first few captured events for a real-time search</h>
          </div>
          <div id="eventsViewer"/>
        </body>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>Number of Events per Manufacturer&amp;Model</title>
      <table>
        <search>
          <query>index="hospital_data" | chart count by "Model" Manufacturer</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">false</option>
        <drilldown>
          <link target="_blank">search?q=index=%22hospital_data%22%20$click.value$%20$click.name2$&amp;earliest=0&amp;latest=</link>
        </drilldown>
      </table>
    </panel>
  </row>
